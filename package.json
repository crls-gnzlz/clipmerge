{
  "name": "clipchain",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node server/server.js",
    "server:dev": "nodemon server/server.js",
    "db:init": "node server/scripts/initDatabase.js",
    "db:seed": "node server/scripts/seedDatabase.js",
    "db:clear": "node server/scripts/clearDatabase.js",
    "db:clear:all": "node server/scripts/clearDatabase.js --drop-database",
    "db:test": "node server/scripts/testConnection.js",
    "db:reset": "npm run db:clear && npm run db:seed",
    "db:setup": "npm run db:init && npm run db:seed",
    "logs:view": "node server/scripts/viewLogs.js",
    "logs:today": "node server/scripts/viewLogs.js --today",
    "logs:yesterday": "node server/scripts/viewLogs.js --yesterday",
    "logs:errors": "node server/scripts/viewLogs.js --errors",
    "logs:stats": "node server/scripts/viewLogs.js --stats",
    "logs:cleanup": "node server/scripts/cleanupLogs.js",
    "logs:cleanup:dry": "node server/scripts/cleanupLogs.js --dry-run",
    "logs:size": "node server/scripts/cleanupLogs.js --size",
    "start": "npm run server:dev & sleep 5 && npm run dev",
    "start:full": "concurrently --names \"Backend,Frontend\" --prefix-colors \"blue,green\" --kill-others \"npm run server:dev\" \"sleep 5 && npm run dev\""
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "googleapis": "^155.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.17",
    "vite": "^7.0.4"
  }
}
